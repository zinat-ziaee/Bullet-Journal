{
    "name": "laravel/laravel",
    "type": "prpject",
    "description": "The Laravel Framework.",
    "keywords": [
    "framework",
    "laravel"
    ],
    "license": "MIT",
    "require": {
      "php": "^7.3|^8.1",
      "laravel/framework": "^8.0",
      "morilog/jalali": "3.*",
      "laravel/ui": "^3.4",
      "fideloper/proxy": "^4.4",
      "yajra/laravel-datatables-oracle": "^9.0",
      "laravel/tinker": "^2.7"
    },
    "autoload": {
        "psr-4": {
          "App\\": "app/",
          "Database\\Seeders\\": "database/seeders/"
        }
    },
    "config": {
      "platform-check": false,
      "platform": {
          "php": "8.1.0"
      }
    },
    "authors": [
        {
            "name": "ziaee"
        }
    ]
}
